<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
    "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<!--留言类别-->
	<package name="auxiliary.msgCtg" namespace="/admin/auxiliary/msg_ctg" extends="core-default">
		<action name="Com_*" method="{1}" class="auxiliary.msgCtgAct">
			<result name="list">/WEB-INF/auxiliary_sys/msg_ctg/list.html</result>
			<result name="add">/WEB-INF/auxiliary_sys/msg_ctg/add.html</result>
			<result name="edit">/WEB-INF/auxiliary_sys/msg_ctg/edit.html</result>
		</action>
	</package>
	<!--留言内容-->
	<package name="auxiliary.msg" namespace="/admin/auxiliary/msg" extends="core-default">
		<action name="Com_*" method="{1}" class="auxiliary.msgAct">
			<result name="list">/WEB-INF/auxiliary_sys/msg/list.html</result>
			<result name="add">/WEB-INF/auxiliary_sys/msg/add.html</result>
			<result name="edit">/WEB-INF/auxiliary_sys/msg/edit.html</result>
		</action>
	</package>
	<!--辅助系统设置-->
	<package name="auxiliary.auxiConfig" namespace="/admin/auxiliary/config" extends="core-default">
		<action name="Com_*" method="{1}" class="auxiliary.auxiConfigAct">
			<result name="edit">/WEB-INF/auxiliary_sys/config/edit.html</result>
		</action>
	</package>
	<!--投票主题管理-->
	<package name="auxiliary.voteTopic" namespace="/admin/auxiliary/vote_topic" extends="core-default">
		<action name="Com_*" method="{1}" class="auxiliary.voteTopicAct">
			<result name="list">/WEB-INF/auxiliary_sys/vote_topic/list.html</result>
			<result name="add">/WEB-INF/auxiliary_sys/vote_topic/add.html</result>
			<result name="edit">/WEB-INF/auxiliary_sys/vote_topic/edit.html</result>
		</action>
	</package>
	<!--献血预约-->
	<package name="auxiliary.booking" namespace="/admin/auxiliary/booking" extends="core-default">
		<action name="Com_*" method="{1}" class="auxiliary.bookingAct">
			<result name="list">/WEB-INF/auxiliary_sys/booking/list.html</result>
		</action>
	</package>
</struts>

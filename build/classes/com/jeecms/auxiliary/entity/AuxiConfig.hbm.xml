<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.auxiliary.entity">
	<class
		name="AuxiConfig"
		table="AUXI_CONFIG"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id
			name="id"
			type="java.lang.Long"
			column="CONFIG_ID"
		>
			<generator class="foreign">
				<param name="property">website</param>
			</generator>
		</id>
		<property
			name="msgNeedCheck"
			column="MSG_NEED_CHECK"
			type="java.lang.Boolean"
			not-null="false"
		/>
		<property
			name="msgIsOpen"
			column="MSG_IS_OPEN"
			type="java.lang.Boolean"
			not-null="false"
		/>
		<property
			name="msgAnonymity"
			column="MSG_ANONYMITY"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="msgMaxSize"
			column="MSG_MAX_SIZE"
			type="integer"
			not-null="false"
			length="10"
		/>
		<one-to-one
			name="website"
			class="com.jeecms.core.entity.Website"
			constrained="true"
			cascade="all"
		/>
	</class>
</hibernate-mapping>
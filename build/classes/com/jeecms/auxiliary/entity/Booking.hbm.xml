<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.auxiliary.entity">
	<class
		name="Booking"
		table="BOOKING"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id
			name="id"
			type="java.lang.Long"
			column="BOOKING_ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="papersType"
			column="PAPERS_TYPE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="papersNumber"
			column="PAPERS_NUMBER"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="sex"
			column="SEX"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="birthday"
			column="BIRTHDAY"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="donateBloodType"
			column="DONATE_BLOOD_TYPE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="bloodType"
			column="BLOOD_TYPE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="mobile"
			column="MOBILE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="telephone"
			column="TELEPHONE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="qq"
			column="QQ"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="place"
			column="PLACE"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="placeName"
			column="PLACE_NAME"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="startDate"
			column="START_DATE"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="endDate"
			column="END_DATE"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="checkTime"
			column="CHECK_TIME"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="check"
			column="IS_CHECK"
			type="java.lang.Boolean"
			not-null="true"
		/>
		<property
			name="appState"
			column="IS_APP_STATE"
			type="int"
			not-null="true"
		/>
		<property
			name="errInfo"
			column="ERR_INFO"
			type="string"
			not-null="false"
			length="500"
		/>
		<many-to-one
			name="website"
			column="WEBSITE_ID"
			class="com.jeecms.core.entity.Website"
			not-null="true"
		>
		</many-to-one>
	</class>	
</hibernate-mapping>
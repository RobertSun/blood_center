<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.core.entity">
	<class
		name="Website"
		table="CORE_WEBSITE"
		dynamic-update="true"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id
			name="id"
			type="java.lang.Long"
			column="WEBSITE_ID"
		>
			<generator class="native"/>
		</id>
		<property
			name="domain"
			column="DOMAIN"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="resPath"
			column="RES_PATH"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="lft"
			column="LFT"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="rgt"
			column="RGT"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="resDomain"
			column="RES_DOMAIN"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="baseDomain"
			column="BASE_DOMAIN"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="domainAlias"
			column="DOMAIN_ALIAS"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="name"
			column="NAME"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="shortName"
			column="SHORT_NAME"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="suffix"
			column="SUFFIX"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="currentSystem"
			column="CURRENT_SYSTEM"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="cookieKey"
			column="COOKIE_KEY"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="ownerName"
			column="OWNER_NAME"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="ownerIdentity"
			column="OWNER_IDENTITY"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="company"
			column="COMPANY"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="copyright"
			column="COPYRIGHT"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="recordCode"
			column="RECORD_CODE"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="email"
			column="EMAIL"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="phoneCode"
			column="PHONE_CODE"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="mobileCode"
			column="MOBILE_CODE"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="true"
		/>
		<component name="emailSender" class="EmailSender">
		<property
			name="charset"
			column="EMAIL_CHARSET"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="hostname"
			column="EMAIL_HOSTNAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="account"
			column="EMAIL_ACCOUNT"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="userName"
			column="EMAIL_USER_NAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="userId"
			column="EMAIL_USER_ID"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="userPwd"
			column="EMAIL_USER_PWD"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="subject"
			column="EMAIL_SUBJECT"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="content"
			column="EMAIL_CONTENT"
			type="string"
			not-null="false"
		/>
		</component>
		<component name="control" class="Control">
		<property
			name="frontIps"
			column="CONTROL_FRONT_IPS"
			type="string"
			not-null="false"
		/>
		<property
			name="adminIps"
			column="CONTROL_ADMIN_IPS"
			type="string"
			not-null="false"
		/>
		<property
			name="reservedWords"
			column="CONTROL_RESERVED"
			type="string"
			not-null="false"
		/>		
		<property
			name="nameMinLen"
			column="CONTROL_NAME_MINLEN"
			type="integer"
			not-null="true"
			length="10"
		/>
		</component>
		<property
			name="closeReason"
			column="CLOSE_REASON"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="close"
			column="IS_CLOSE"
			type="java.lang.Boolean"
			not-null="true"
		/>
		<many-to-one
			name="parent"
			column="PARENT"
			class="Website"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="global"
			column="GLOBAL_ID"
			class="Global"
			not-null="true"
		>
		</many-to-one>
		<set name="child" inverse="true">
			<cache usage="read-write"/>
			<key column="PARENT"/>
			<one-to-many class="Website"/>
		</set>
		<map name="solutions" table="CORE_TPL_SOLUTION" cascade="all-delete-orphan">
			<cache usage="read-write"/>
			<key column="WEBSITE_ID" not-null="true"/>
			<map-key column="SYSTEM_NAME" type="string" length="20"/>
			<element column="SOLUTION_NAME" type="string" length="20"/>
		</map>
	</class>	
</hibernate-mapping>
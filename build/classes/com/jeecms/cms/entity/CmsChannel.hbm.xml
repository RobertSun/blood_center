			<cache usage="read-write"/>
			<key column="PARENT"/>
			<one-to-many class="CmsChannel"/>
			<filter name="channelFilter" condition="HAS_CHILD=1"/>
		</set>
		<set
			name="attachments"
			table="CMS_CHANNEL_ATTCHMENT"
			cascade="all-delete-orphan"
		>
			<cache usage="read-write"/>
			<key column="CHANNEL_ID"/>
			<many-to-many column="ATTACHMENT_ID" class="com.jeecms.core.entity.Attachment"/>
		</set>
		<set
			name="admins"
			table="CMS_ADMIN_CHANNEL"
			inverse="true"
		>
			<key column="CHANNEL_ID"/>
			<many-to-many column="ADMIN_ID" class="CmsAdmin"/>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.cms.entity">
	<class
		name="ChnlModelItem"
		table="CMS_CHNL_MODEL_ITEM"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id
			name="id"
			type="java.lang.Long"
			column="MITEM_ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="itemType"
			column="MITEM_TYPE"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="name"
			column="NAME"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="label"
			column="LABEL"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="help"
			column="HELP"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="dataType"
			column="DATA_TYPE"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="inputType"
			column="INPUT_TYPE"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="inputSize"
			column="INPUT_SIZE"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="inputWidth"
			column="INPUT_WIDTH"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="textareaCols"
			column="TEXTAREA_COLS"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="textareaRows"
			column="TEXTAREA_ROWS"
			type="integer"
			not-null="false"
			length="10"
		/>
		
		<property
			name="defValue"
			column="DEF_VALUE"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="optionValue"
			column="OPTION_VALUE"
			type="string"
			not-null="false"
			length="255"
		/>		
		<property
			name="priority"
			column="PRIORITY"
			type="integer"
			not-null="true"
			length="10"
			/>
		<property
			name="predefine"
			column="IS_PREDEFINE"
			type="java.lang.Boolean"
			not-null="true"
		/>
		<property
			name="checked"
			column="IS_CHECKED"
			type="java.lang.Boolean"
			not-null="true"
		/>
		<many-to-one
			name="model"
			column="MODEL_ID"
			class="ChnlModel"
			not-null="true"
		>
		</many-to-one>
	</class>
</hibernate-mapping>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="bean.id">
		<field-validator type="required">
			<message>该项不能为空</message>
		</field-validator>
	</field>
	<field name="uploadRuleId">
		<field-validator type="required">
			<message>该项不能为空</message>
		</field-validator>
	</field>
	<field name="bean.channel.id">
		<field-validator type="required">
			<message>该项不能为空</message>
		</field-validator>
	</field>
	<field name="bean.content">
		<field-validator type="requiredstring">
			<message>该项不能为空</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">1048576</param>
			<param name="trim">false</param>
			<message>长度不能超过1048576(2M)个字符</message>
		</field-validator>
	</field>
	<field name="bean.title">
		<field-validator type="requiredstring">
			<message>该项不能为空</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">255</param>
			<param name="trim">false</param>
			<message>长度不能超过255个字符</message>
		</field-validator>
	</field>
	<field name="bean.description">
		<field-validator type="stringlength">
			<param name="maxLength">255</param>
			<param name="trim">false</param>
			<message>长度不能超过255个字符</message>
		</field-validator>
	</field>
	<field name="bean.author">
		<field-validator type="stringlength">
			<param name="maxLength">100</param>
			<param name="trim">false</param>
			<message>长度不能超过100个字符</message>
		</field-validator>
	</field>
	<field name="bean.origin">
		<field-validator type="stringlength">
			<param name="maxLength">100</param>
			<param name="trim">false</param>
			<message>长度不能超过100个字符</message>
		</field-validator>
	</field>
	<field name="bean.tags">
		<field-validator type="stringlength">
			<param name="maxLength">255</param>
			<param name="trim">false</param>
			<message>长度不能超过255个字符</message>
		</field-validator>
	</field>
</validators>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.core.entity">
	<class
		name="Attachment"
		table="CORE_ATTACHMENT"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id
			name="id"
			type="java.lang.Long"
			column="ATTACHMENT_ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="filePath"
			column="FILE_PATH"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="fileName"
			column="FILE_NAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="fileSize"
			column="FILE_SIZE"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="downCount"
			column="DOWN_COUNT"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="ownerName"
			column="OWNER_NAME"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="ownerUrl"
			column="OWNER_URL"
			type="string"
			not-null="false"
			length="150"
		/>
		<property
			name="ownerId"
			column="OWNER_ID"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="ownerCtg"
			column="OWNER_CTG"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="free"
			column="IS_FREE"
			type="java.lang.Boolean"
			not-null="true"
		/>
		<property
			name="lost"
			column="IS_LOST"
			type="java.lang.Boolean"
			not-null="true"
		/>
		<property
			name="picture"
			column="IS_PICTURE"
			type="java.lang.Boolean"
			not-null="false"
		/>
		<many-to-one
			name="website"
			column="WEBSITE_ID"
			class="Website"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="user"
			column="USER_ID"
			class="User"
			not-null="false"
		>
		</many-to-one>
	</class>	
</hibernate-mapping>
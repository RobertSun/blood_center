<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.auxiliary.entity">
	<class
		name="VoteRecord"
		table="VOTE_RECORD"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="java.lang.Long"
			column="VRECORD_ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="voteTime"
			column="VOTE_TIME"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="voteIp"
			column="VOTE_IP"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="voteCookie"
			column="VOTE_COOKIE"
			type="string"
			not-null="false"
			length="32"
		/>
		<many-to-one
			name="topic"
			column="VTOPIC_ID"
			class="VoteTopic"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="member"
			column="MEMBER_ID"
			class="com.jeecms.core.entity.Member"
			not-null="false"
		>
		</many-to-one>
	</class>	
</hibernate-mapping>